<?php
 $path = "../assets/uploads/";
  $file="";
  $msg='file upload success post';
  $returnArr["errCode"] =1 ;
  $returnArr["errMsg"] = $msg;
  $name = $_FILES['file1']['name'];
  $size = $_FILES['file1']['size'];
  list($txt, $ext) = explode(".", $name);
      $actual_file_name = time().substr(str_replace(" ", "_", $txt), 5).".".$ext;
      $tmp = $_FILES['file1']['tmp_name'];
      	if(move_uploaded_file($tmp, $path.$actual_file_name)){
        	    
        	//mysqli_query($conn,"UPDATE admin_contact_doctor SET image='$actual_file_name' WHERE doctorId=".$_POST['userId']);
              
            //echo "<img style='height:100px;'' src='../../assets/uploads/".$actual_file_name."' class='preview'>";
            $file=$actual_file_name;
            $msg='file upload success';
            $returnArr["errCode"] =1 ;
            $returnArr["errMsg"] = $msg;
          }
      	else{
            $msg="failed file upload";
            $returnArr["errCode"] =5 ;
